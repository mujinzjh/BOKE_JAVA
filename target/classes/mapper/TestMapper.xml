<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="cn.tycoding.biz.mapper.TestMapper">

    <select id="findByTestId" parameterType="long" resultType="cn.tycoding.biz.entity.TestEntity">
        SELECT * FROM tb_test t
        WHERE t.id = #{id}
    </select>

</mapper>
